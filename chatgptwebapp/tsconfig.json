{
  "compileOnSave": true,
  "strictNullChecks": true,
  "strictPropertyInitialization": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES2016",
    "module": "ES6",
    "strict": true,
    "moduleResolution":"node",
    "forceConsistentCasingInFileNames": true,
    "lib": [ "ES2015", "DOM" ]
  },
  "include": [
    "app/static/app/html/*",
    "app/static/app/scripts/app/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
